<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <Title>Twino.Server</Title>
        <Product>Twino.Server</Product>
        <Description>Twino TCP Socket Server</Description>
        <PackageTags>twino,tcp,socket,server,http,websocket,tmq,mq</PackageTags>
        <AssemblyVersion>2.4.2</AssemblyVersion>
        <FileVersion>2.4.2</FileVersion>
        <PackageVersion>2.4.2</PackageVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Mehmet Helvacıköylü</Authors>
        <PackageProjectUrl>https://github.com/mhelvacikoylu/twino</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/mhelvacikoylu/twino/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/mhelvacikoylu/twino</RepositoryUrl>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DocumentationFile>bin\Debug\Twino.Server.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Twino.Server.xml</DocumentationFile>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\Twino.Core\Twino.Core.csproj" />
    </ItemGroup>

</Project>
