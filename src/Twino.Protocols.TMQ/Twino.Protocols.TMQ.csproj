<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Title>Twino.Protocols.TMQ</Title>
    <Product>Twino.Protocols.TMQ</Product>
    <Description>Twino Messaging Queue (TMQ) Protocol library and server extension for Twino Server</Description>
    <PackageTags>twino,tcp,server,http,messaging,queue,tmq,mq,protocol</PackageTags>
    <AssemblyVersion>2.8.1</AssemblyVersion>
    <FileVersion>2.8.1</FileVersion>
    <PackageVersion>2.8.1</PackageVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Mehmet Helvacıköylü;Emre Hızlı</Authors>
    <PackageProjectUrl>https://github.com/mhelvacikoylu/twino</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/mhelvacikoylu/twino/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/mhelvacikoylu/twino</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\Twino.Protocols.TMQ.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\Twino.Protocols.TMQ.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Twino.Core\Twino.Core.csproj" />
  </ItemGroup>

</Project>
