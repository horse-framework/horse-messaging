<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <Title>Twino.Client.WebSocket</Title>
        <Product>Twino.Client.WebSocket</Product>
        <Description>Twino WebSocket Client to connect all WebSocket servers</Description>
        <PackageTags>twino,websocket,client</PackageTags>
        <AssemblyVersion>2.3.4</AssemblyVersion>
        <FileVersion>2.3.4</FileVersion>
        <PackageVersion>2.3.4</PackageVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>Mehmet Helvacıköylü</Authors>
        <PackageProjectUrl>https://github.com/mhelvacikoylu/twino</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/mhelvacikoylu/twino/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/mhelvacikoylu/twino</RepositoryUrl>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DocumentationFile>bin\Debug\Twino.Client.WebSocket.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Twino.Client.WebSocket.xml</DocumentationFile>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\Twino.Client.Connectors\Twino.Client.Connectors.csproj" />
        <ProjectReference Include="..\Twino.Protocols.WebSocket\Twino.Protocols.WebSocket.csproj" />
    </ItemGroup>

</Project>
